        <body>
          <ST>
            <xhtml xmlns="http://www.w3.org/1999/xhtml">// Communication
// ToPlc
counterValue := DWORD_TO_UINT(ModelToPLC AND 16#00_00_FF_FF);
driveInState1 := DWORD_TO_USINT(SHR((ModelToPLC AND 16#00_FF_00_00),16));


// ToModel
PLCToModel := 0;
PLCToModel := SHL((PLCToModel OR velocity),0);</xhtml>
          </ST>
        </body>
