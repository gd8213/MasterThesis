    <pous>
      <pou name="MAIN" pouType="program">
        <interface>
          <localVars>
            <variable name="PosDegree" address="%I*">
              <type>
                <REAL />
              </type>
              <initialValue>
                <simpleValue value="0" />
              </initialValue>
              <documentation>
                <xhtml xmlns="http://www.w3.org/1999/xhtml"> in deg</xhtml>
              </documentation>
            </variable>
            <variable name="PosPiston" address="%I*">
              <type>
                <REAL />
              </type>
              <initialValue>
                <simpleValue value="0" />
              </initialValue>
              <documentation>
                <xhtml xmlns="http://www.w3.org/1999/xhtml"> in mm</xhtml>
              </documentation>
            </variable>
            <variable name="velocity" address="%Q*">
              <type>
                <REAL />
              </type>
              <initialValue>
                <simpleValue value="0" />
              </initialValue>
              <documentation>
                <xhtml xmlns="http://www.w3.org/1999/xhtml"> in deg/s</xhtml>
              </documentation>
            </variable>
            <variable name="cycleTime">
              <type>
                <REAL />
              </type>
              <initialValue>
                <simpleValue value="0.01" />
              </initialValue>
              <documentation>
                <xhtml xmlns="http://www.w3.org/1999/xhtml"> in sec</xhtml>
              </documentation>
            </variable>
          </localVars>
        </interface>
        <body>
          <ST>
            <xhtml xmlns="http://www.w3.org/1999/xhtml">// Position of Disc
PosDegree := PosDegree + velocity*cycleTime;
WHILE PosDegree &gt; 360 DO
	PosDegree := PosDegree -360;
END_WHILE


// Position of Piston    
PosPiston := 13 + COS(PosDegree*pi/180)*9 + SQRT(EXPT(25,2)-EXPT((SIN(PosDegree*pi/180 )*9),2));

</xhtml>
          </ST>
        </body>
        <addData>
          <data name="http://www.3s-software.com/plcopenxml/interfaceasplaintext" handleUnknown="implementation">
            <InterfaceAsPlainText>
              <xhtml xmlns="http://www.w3.org/1999/xhtml">PROGRAM MAIN
VAR	
	PosDegree AT %I*	: REAL	:= 0;		// in deg
	PosPiston AT %I*	: REAL	:= 0;		// in mm
	velocity AT %Q*		: REAL	:= 0;		// in deg/s
	
	cycleTime 			: REAL := 0.01;		// in sec
END_VAR
</xhtml>
            </InterfaceAsPlainText>
          </data>
